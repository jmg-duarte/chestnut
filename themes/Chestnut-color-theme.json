  "semanticHighlighting": true,
  "semanticTokenColors": {
    // Having `"fontStyle": ""` will override the regular tokens!!
    "class": {
      "foreground": "#40777F",
      "fontStyle": ""
    },
    "struct": {
      "foreground": "#40777F",
      "fontStyle": ""
    },
    "enum": {
      "foreground": "#40777F",
      "fontStyle": ""
    },
    "enumMember": {
      "foreground": "#40777F",
      "fontStyle": ""
    },
    // Types
    "type": {
      "foreground": "#40777F"
    },
    "typeAlias": {
      "foreground": "#40777F"
    },
    "*.typeHint": {
      "foreground": "#40777F"
    },
    "interface": {
      "foreground": "#40777F"
    },
    // Macro related
    "macro": {
      "foreground": "#6636C6"
    },
    "builtinAttribute": {
      "foreground": "#6636C6"
    },
    "decorator": {
      "foreground": "#6636C6"
    },
    "deriveHelper": {
      "foreground": "#6636C6"
    },
    "attribute": "#6636C6",
    "attributeBracket": "#6636C6",
    // Functions / Methods
    "method": {},
    "function": {},
    "magicFunction": {
      "fontStyle": "italic"
    },
    // Function Parameters
    "parameter": {
      "fontStyle": ""
    },
    "escapeSequence": {
      "foreground": "#c54029",
      "fontStyle": "bold"
    },
    "keyword": {
      "foreground": "#2F3DA3"
    },
    "namespace": {
      "fontStyle": ""
    },
    "operator": {
      "fontStyle": ""
    },
    "*.controlFlow": {
      "fontStyle": "bold"
    },
    "*.unsafe": {
      "fontStyle": "bold"
    },
    "*.consuming": {
      "fontStyle": "italic"
    },
    "*.mutable": {
      "fontStyle": "underline"
    },
    // They're usually upper case
    "*.constant": {
      "fontStyle": ""
    },
    "builtinConstant": {
      "fontStyle": ""
    },
    "*.builtin": {
      "fontStyle": ""
    },
    "*.deprecated": {
      "fontStyle": "strikethrough"
    },
    "comment": "#6d6d6d",
    "label": {
      "fontStyle": "bold"
    },
    "string": "#266939"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        // "fontStyle": "italic",
        "foreground": "#6d6d6d"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted.single.python"
      ],
      "settings": {
        "foreground": "#266939",
        "fontStyle": "" // Empty to enforce no style
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": [
        "constant.character.escape",
        "support.other.escape.special"
      ],
      "settings": {
        "foreground": "#c54029",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Constant Literals",
      "scope": [
        "constant.numeric",
        "constant.language.json.comments"
      ],
      "settings": {
        "foreground": "#2F3DA3"
      }
    },
    {
      "name": "YAML: True, False, Null",
      "scope": [
        "constant.language.boolean.yaml",
        "constant.language.null.yaml"
      ],
      "settings": {
        "foreground": "#2F3DA3"
      }
    },
    {
      "name": "YAML: Name Tag",
      "scope": [
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#40777F"
      }
    },
    {
      "name": "Fallback: Enum & Enum Member",
      "scope": [
        "entity.name.type.enum",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#40777F",
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback: Namespaces",
      "scope": "entity.name.namespace",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback: Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "storage.type"
      ],
      "settings": {
        "foreground": "#40777F",
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback: Struct",
      "scope": [
        "storage.type.struct"
      ],
      "settings": {
        "foreground": "#40777F",
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback: Class",
      "scope": [
        "entity.name.type.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#40777F",
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback: Interface",
      "scope": [
        "entity.name.type.interface"
      ],
      "settings": {
        "foreground": "#40777F",
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback: Function",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.function.member"
      ],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback: Macro",
      "scope": [
        "entity.name.function.macro"
      ],
      "settings": {
        "foreground": "#6636C6"
      }
    },
    {
      "name": "Fallback: Variable",
      "scope": [
        "variable.other.readwrite",
        "entity.name.variable",
        "variable.other.constant",
        "support.constant"
      ],
      "settings": {}
    },
    {
      "name": "Fallback: Parameter",
      "scope": [
        "variable.parameter"
      ],
      "settings": {}
    },
    {
      "name": "Fallback: Property",
      "scope": [
        "variable.parameter",
        "variable.other.parameter"
      ],
      "settings": {}
    },
    {
      "name": "Fallback: Event",
      "scope": [
        "variable.other.event"
      ],
      "settings": {}
    },
    {
      "name": "Fallback: Keywords",
      "scope": [
        "keyword.function",
        "keyword.control",
        "keyword.var",
        "keyword.type",
        "keyword.package",
        "keyword.interface"
      ],
      "settings": {
        "foreground": "#2F3DA3"
      }
    },
    {
      "name": "Python: Decorators",
      "scope": [
        "punctuation.definition.decorator.python",
        "entity.name.function.decorator.python",
        "meta.function.decorator.python"
      ],
      "settings": {
        "foreground": "#6636C6"
      }
    },
    {
      "name": "Python Keyword: Declarations",
